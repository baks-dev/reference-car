{
  "name": "baks-dev/reference-car",
  "description": "Библиотека автомобилей",
  "homepage": "https://github.com/baks-dev/reference-car",
  "keywords": [
    "bundle",
    "symfony",
    "library",
    "reference",
    "car",
    "brans",
    "baks"
  ],
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Baks Development",
      "email": "admin@baks.dev"
    }
  ],
  "require": {
    "php": ">=8.4",
    "baks-dev/core": "^7.4"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "BaksDev\\Reference\\Car\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BaksDev\\Reference\\Car\\": "Tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}